import math as m
t = 1
x = 10

a = 9 * 3.14 * t
b = 10* m.cos(x)
c = m.sqrt(t)
d = m.fabs(m.sin(x))
f = m.exp(x)

Z = ((a + b) / (c - d)) * f
print(Z)

number = int(input("Input num: "))
i = 1
factorial = 1
while i <= number:
  factorial *= i #Это то же самое, что и factorial = factorial * i
  i += 1         #Тут также, i = i + 1
print("Factorial of", str(number), "is", str(factorial))